      },
      "Concurrent_Sessions": {
        "type": "integer"
      },
      "Request_Limit_Daily": {
        "type": "integer"
      },
      "Session_Cap": {
        "type": "integer"
      },
      "Session_Time_Limit": {
        "type": "integer",
