{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://api.paladins.com/paladinsapi.svc/getmatchplayerdetailsjson",
  "type": "array",
  "title": "'get-match-player-details' schema",
  "description": "Schema for paladins 'getmatchplayerdetailsJSON' endpoint",
  "default": {},
  "maxLength": 10,
  "items": {
    "type": "object",
    "properties": {
      "ret_msg": {
        "type": [
          "string",
          "null"
        ],
        "examples": [
          "Approved",
          "Invalid Signature"
        ]
      },
      "Account_Champions_Played": {
        "type": "number",
        "minimum": 0
      },
      "Account_Level": {
        "type": "number",
        "minimum": 0,
        "maximum": 999
      },
      "ChampionId": {
        "type": "number"
      },
      "ChampionLevel": {
        "type": "number",
        "minimum": 0,
        "maximum": 999
      },
      "ChampionName": {
        "type": "string"
      },
      "Mastery_Level": {
        "type": "number"
      },
      "Match": {
        "type": "number"
      },
      "Queue": {
        "type": "string"
      },
      "Skin": {
        "type": "string"
      },
      "SkinId": {
        "type": "number"
      },
      "Tier": {
        "type": "number"
      },
      "mapGame": {
        "type": "string"
      },
      "playerCreated": {
        "type": "string",
        "pattern": "^(\\d{1,2}\\/\\d{1,2}\\/\\d{4})\\s(0?[1-9]|1[0-2]):([0-5]\\d):([0-5]?\\d)\\s?([APap]\\.?[Mm]\\.?)$",
        "examples": [
          "2/3/2018 12:51:29 AM",
          "3/17/2018 8:01:25 AM"
        ]
      },
      "playerId": {
        "type": "string"
      },
      "playerName": {
        "type": "string"
      },
      "playerPortalId": {
        "type": "string"
      },
      "playerPortalUserId": {
        "type": "string"
      },
      "playerRegion": {
        "type": "string"
      },
      "taskForce": {
        "type": "number",
        "minimum": 1,
        "maximum": 2
      },
      "tierLosses": {
        "type": "number"
      },
      "tierWins": {
        "type": "number"
      }
    },
    "required": [
      "Account_Champions_Played",
      "Account_Level",
      "ChampionId",
      "ChampionLevel",
      "ChampionName",
      "Mastery_Level",
      "Match",
      "Queue",
      "Skin",
      "SkinId",
      "Tier",
      "mapGame",
      "playerCreated",
      "playerId",
      "playerName",
      "playerPortalId",
      "playerPortalUserId",
      "playerRegion",
      "ret_msg",
      "taskForce",
      "tierLosses",
      "tierWins"
    ],
    "additionalProperties": false
  },
