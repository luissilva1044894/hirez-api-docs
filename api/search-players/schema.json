      "type": "object",
      "properties": {
        "ret_msg": {
          "type": [
            "string",
            "null"
          ],
          "examples": [
            "Approved",
            "Invalid Signature"
          ]
        },
        "Name": {
          "type": "string"
        },
        "player_id": {
          "type": "string"
        },
        "hz_player_name": {
          "type": [
            "string",
            "null"
          ]
        },
        "portal_id": {
          "type": "string"
        },
        "privacy_flag": {
          "type": "string"
        }
      },
      "required": [
        "Name",
        "hz_player_name",
        "player_id",
        "portal_id",
        "privacy_flag",
        "ret_msg"
      ],
      "additionalProperties": false
    },
