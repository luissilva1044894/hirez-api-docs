{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://api.paladins.com/paladinsapi.svc/searchplayersjson",
    "type": "array",
    "title": "'search-players' schema",
    "description": "Schema for paladins/smite 'searchplayersJSON' endpoint",
